package bean;

import java.util.Date;
import java.util.HashMap;
import java.util.LinkedList;

/**
 *
 * @author Administrator
 */
public class Stock {
    private LinkedList stockList;
    private double[] high;
    private double[] low;
    private double[] close;
    private double[] open;
    private double[] volume;
    private Date[] dateTime;
    private HashMap indicators;
    private String name;
    private int emaSignal;

    
    
    public Date[] getDateTime() {
        return dateTime;
    }

    public void setDateTime(Date[] dateTime) {
        this.dateTime = dateTime;
    }
    public int getEmaSignal() {
        return emaSignal;
    }

    public void setEmaSignal(int emaSignal) {
        this.emaSignal = emaSignal;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public double[] getClose() {
        return close;
    }

    public void setClose(double[] close) {
        this.close = close;
    }


    public double[] getHigh() {
        return high;
    }

    public void setHigh(double[] high) {
        this.high = high;
    }

    public double[] getLow() {
        return low;
    }

    public void setLow(double[] low) {
        this.low = low;
    }

    public double[] getOpen() {
        return open;
    }

    public void setOpen(double[] open) {
        this.open = open;
    }

    public LinkedList getStockList() {
        return stockList;
    }

    public void setStockList(LinkedList stockList) {
        this.stockList = stockList;
    }

    public double[] getVolume() {
        return volume;
    }

    public void setVolume(double[] volume) {
        this.volume = volume;
    }

    /**
     * @return the indicators
     */
    public HashMap getIndicators() {
        return indicators;
    }

    /**
     * @param indicators the indicators to set
     */
    public void setIndicators(HashMap indicators) {
        this.indicators = indicators;
    }
}
